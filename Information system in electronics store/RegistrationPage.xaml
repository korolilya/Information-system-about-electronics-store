<Page x:Class="Information_system_in_electronics_store.RegistrationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Information_system_in_electronics_store"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="600"
      Title="RegistrationPage">

    <Grid>
        <TextBox x:Name="textBoxName" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Margin="0,196,0,0"/>
        <TextBox x:Name="textBoxPassword" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Margin="0,292,0,0"/>
        <Button x:Name="buttonConfirm" Content="Зарегистрироваться" HorizontalAlignment="Center" Margin="0,373,0,0" VerticalAlignment="Top" Width="120" Click="buttonConfirm_Click"/>
        <TextBlock x:Name="textBlockName" HorizontalAlignment="Center" Margin="-150,197,0,0" TextWrapping="Wrap" Text="Логин:" VerticalAlignment="Top" Height="23" Width="60"/>
        <TextBlock x:Name="textBlockPasswrd" HorizontalAlignment="Center" Margin="-160,293,0,0" TextWrapping="Wrap" Text="Пароль:" VerticalAlignment="Top" Height="23" Width="60"/>

    </Grid>
</Page>
