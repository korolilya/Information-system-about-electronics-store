<Window x:Class="Information_system_in_electronics_store.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Information_system_in_electronics_store"
        mc:Ignorable="d"
        Title="LogInWindow" Height="300" Width="300" Background="AntiqueWhite">
    <Grid>
        <Button x:Name="LogInbutton" Content="Войти" HorizontalAlignment="Left" Margin="108,166,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.473,0.612" Click="button_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="73,116,0,0" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="UsernameBox" HorizontalAlignment="Left" Height="22" Margin="73,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="150"/>
    </Grid>
</Window>
